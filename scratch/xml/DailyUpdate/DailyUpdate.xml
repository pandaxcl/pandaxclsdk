<?xml version="1.0"?>
<?xml-stylesheet type="text/xsl" href="DailyUpdate.xsl"?>
<DailyUpdate>
  <class name="DailyUpdate" tableName="daily_updates">
    <base_class name="Uploadable"/>
    <attribute name="user_id" objc_p="nonatomic,copy" objc_t="NSString*" sql_t="VARCHAR(36) NULL"/>
    <attribute name="name" objc_p="nonatomic,copy" objc_t="NSString*" sql_t="VARCHAR(255) NULL"/>
    <attribute name="description" objc_p="nonatomic,copy" objc_t="NSString*" sql_t="TEXT NULL"/>
    <attribute name="status" objc_p="nonatomic,copy" objc_t="NSString*" sql_t="VARCHAR(16) NULL">
      <enum>
        <item>Open</item>
        <item>Closed</item>
      </enum>
    </attribute>
    <attribute name="created_user_id" objc_p="nonatomic,copy" objc_t="NSString*" sql_t="VARCHAR(36) NULL"/>
  </class>
  <class name="DailyUpdateSection" tableName="daily_update_sections">
    <base_class name="Uploadable"/>
    <attribute name="daily_update_id" objc_p="nonatomic,copy" objc_t="NSString*" sql_t="VARCHAR(36) NULL"/>
    <attribute name="account_id" objc_p="nonatomic,copy" objc_t="NSString*" sql_t="VARCHAR(36) NULL"/>
    <attribute name="name" objc_p="nonatomic,copy" objc_t="NSString*" sql_t="VARCHAR(255) NULL"/>
    <attribute name="notes" objc_p="nonatomic,copy" objc_t="NSString*" sql_t="TEXT NULL"/>
    <attribute name="company_id" objc_p="nonatomic,copy" objc_t="NSString*" sql_t="VARCHAR(36) NULL"/>
    <attribute name="section_type" objc_p="nonatomic,copy" objc_t="NSString*" sql_t="VARCHAR(255) NULL">
      <enum>
        <item>Labor</item>
        <item>Weather</item>
      </enum>
    </attribute>
  </class>
  <class name="DailyUpdateWeather" tableName="daily_update_weather_sections">
    <base_class name="Uploadable"/>
    <attribute name="daily_update_section_id" objc_p="nonatomic,copy" objc_t="NSString*" sql_t="VARCHAR(36) NULL"/>
    <attribute name="humidity" objc_p="nonatomic,assign" objc_t="CGFloat" sql_t="FLOAT NULL"/>
    <attribute name="icon" objc_p="nonatomic,copy" objc_t="NSString*" sql_t="VARCHAR(255) NULL"/>
    <attribute name="precip_accumulation" objc_p="nonatomic,assign" objc_t="CGFloat" sql_t="FLOAT NULL"/>
    <attribute name="precip_type" objc_p="nonatomic,copy" objc_t="NSString*" sql_t="VARCHAR(32) NULL"/>
    <attribute name="summary" objc_p="nonatomic,copy" objc_t="NSString*" sql_t="TEXT NULL"/>
    <attribute name="temperature_max" objc_p="nonatomic,assign" objc_t="CGFloat" sql_t="FLOAT NULL"/>
    <attribute name="temperature_max_time" objc_p="nonatomic,assign" objc_t="CGFloat" sql_t="FLOAT NULL"/>
    <attribute name="temperature_min" objc_p="nonatomic,assign" objc_t="CGFloat" sql_t="FLOAT NULL"/>
    <attribute name="temperature_min_time" objc_p="nonatomic,assign" objc_t="CGFloat" sql_t="FLOAT NULL"/>
    <attribute name="visibility" objc_p="nonatomic,assign" objc_t="CGFloat" sql_t="FLOAT NULL"/>
    <attribute name="wind_speed" objc_p="nonatomic,assign" objc_t="CGFloat" sql_t="FLOAT NULL"/>
    <attribute name="zipcode" objc_p="nonatomic,copy" objc_t="NSString*" sql_t="TEXT NULL"/>
    <attribute name="progress_affected" objc_p="nonatomic,copy" objc_t="NSString*" sql_t="VARCHAR(255) NULL"/>
  </class>
  <class name="DailyUpdateLabor" tableName="daily_update_labors">
    <base_class name="Uploadable"/>
    <attribute name="daily_update_section_id" objc_p="nonatomic,copy" objc_t="NSString*" sql_t="VARCHAR(36) NULL"/>
    <attribute name="user_id" objc_p="nonatomic,copy" objc_t="NSString*" sql_t="VARCHAR(36) NULL"/>
    <attribute name="company_id" objc_p="nonatomic,copy" objc_t="NSString*" sql_t="VARCHAR(36) NULL"/>
    <attribute name="amount" objc_p="nonatomic,assign" objc_t="CGFloat" sql_t="FLOAT NULL"/>
    <attribute name="notes" objc_p="nonatomic,copy" objc_t="NSString*" sql_t="TEXT NULL"/>
    <attribute name="workers" objc_p="nonatomic,copy" objc_t="NSString*" sql_t="VARCHAR(36) NULL"/>
  </class>
</DailyUpdate>
